.slider-wrapper
  position relative

.slider-nav-remax
  position absolute
  height 90px
  bottom 0
  left 0
  right 0
  padding-left: 15px
  padding-right: 15px
  margin 0 auto

.number
  display block
  float left
  margin 3px
  width 20px
  height 20px
  background blue
  border-radius 50%
  text-align center
  color white
  cursor pointer

.pic-th
  display block
  float left
  margin 3px
  width 70px
  text-align center
  cursor pointer
  background-color white
  border 1px white solid


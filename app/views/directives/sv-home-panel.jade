.one-stop-service
  .container
    fa-app( ng-style='myStyle' )
      fa-scroll-view(fa-pipe-from='handler'  )
        fa-view(ng-repeat='home in homes' fa-size='[{{wsm}},{{hsm}}]')
          fa-modifier(fa-size='[{{wsm}},{{hsm}}]' fa-rotate-x='home.angle.get()' fa-align="[0, 0]" fa-origin="[0, 0]")
            fa-app(fa-perspective="[1000]")
              fa-surface(fa-pipe-to='handler' fa-background-color="'blue'")


.modal
  .modal-dialog
    .modal-content.modal-dialog-wishlist
      .modal-header
        button.close(ng-click='$hide()') &times
        h3.modal-title(ng-bind='title')
      .modal-body.modal-wishlist
        form(name="form" id="form" novalidate)
          .row
            .col-sm-4
              .form-group
                label State
                input.form-control(type='text' ng-model='wishList.state' name='state' id='state' required)
            .col-sm-4
              .form-group
                label City
                input.form-control(type='text' ng-model='wishList.city' name='city' id='city' required)
            .col-sm-4
              .form-group
                label Property Type
                input.form-control(type='text' ng-model='wishList.propertyType' name='propertyType' id='propertyType' required)
          .row
            .col-sm-4
              .form-group
                label Bedrooms

                input.form-control(type='number' ng-model='wishList.bedrooms' name='bedrooms' id='bedrooms' required)
            .col-sm-4
              .form-group
                label Bathrooms
                input.form-control(type='number' ng-model='wishList.bathrooms' name='bathrooms' id='bathrooms' required)
            .col-sm-4
              .form-group
                label Parking Type
                input.form-control(type='text' ng-model='wishList.parkingType' name='parkingType' id='parkingType' required)
          .row
            .col-sm-12
              .form-group
                label Additional Info
                textarea.form-control(ng-model='wishList.addInfo' name='addInfo' id='addInfo')
          .row
            .col-sm-12
              button.btn.btn-primary(ng-click='saveWishList(wishList)' ng-disabled='form.$invalid || form.$pristine')
                i.fa.fa-save.fa-fw
                |Save



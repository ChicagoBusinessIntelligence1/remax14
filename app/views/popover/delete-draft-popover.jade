.popover.broker-app-popover
  .arrow
  h3.popover-title Are you sure you want delete your draft?
  .popover-content
    button.btn.btn-danger.btn-sm.btn-save(ng-click='removeHomeDraft(mls);$hide()' data-toggle="tooltip" data-placement="left" title="{{deleteDraft}}")
      i.fa.fa-check
      | &nbsp Delete
    button.btn.btn-default.btn-sm.btn-cancel(ng-click='$hide()')
      i.fa.fa-times
      | &nbsp Cancel
